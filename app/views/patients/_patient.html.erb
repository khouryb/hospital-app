<div id="<%= dom_id patient %>">
  <p>
    <strong>First name:</strong>
    <%= patient.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= patient.last_name %>
  </p>

  <p>
    <strong>Diagnosis:</strong>
    <%= patient.diagnosis %>
  </p>

  <p>
    <strong>Born on:</strong>
    <%= patient.born_on %>
  </p>

</div>
